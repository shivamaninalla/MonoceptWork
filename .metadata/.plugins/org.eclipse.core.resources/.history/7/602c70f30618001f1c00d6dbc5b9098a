package com.monocept.model;

public class Rectangle {
	private float width;
	private float height;
	private float area;
	public void initialize(float width1,float height1) {
		width=width1;
		height=height1;
	}
	public void display() {
		System.out.println("Width: "+width);
		System.out.println("Height: "+height);
	}
	public void area() {
		area=width*height;
		System.out.println("Area: "+area);
	}

}
