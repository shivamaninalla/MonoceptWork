package com.monocept.model;

public class Rectangle {
	private float width;
	private float height;
	private float area;
	public void initialize(float width,float height1) {
		this.width=width;
		height=height1;
	}
	public void display() {
		System.out.println("Width: "+width);
		System.out.println("Height: "+height);
	}
	public void area(float area1) {
		area=area1;
		System.out.println("Area: "+area);
	}

}
